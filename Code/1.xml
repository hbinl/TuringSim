<?xml version="1.0" ?>
<turingmachine>
  
  
  <alphabet>0123b</alphabet>
  
  
  <initialtape>1010101</initialtape>
  
  
  <blank char="b"/>
  
  
  <initialstate name="0"/>
  
  
  <finalstates>
    
    
    <finalstate name="halt"/>
  </finalstates>
  
  
  <states>
    
    
    <state name="1">
      <transition move="R" newstate="1" seensym="1" writesym="0"/>
      <transition move="N" newstate="2" seensym="0" writesym="1"/>
      <transition move="L" newstate="2" seensym="b" writesym="b"/>
    </state>
    <state name="0">
      <transition move="R" newstate="1" seensym="1" writesym="1"/>
      <transition move="R" newstate="0" seensym="0" writesym="0"/>
      <transition move="L" newstate="2" seensym="b" writesym="b"/>
    </state>
    <state name="2">
      <transition move="L" newstate="2" seensym="1" writesym="3"/>
      <transition move="L" newstate="2" seensym="0" writesym="2"/>
      <transition move="R" newstate="halt" seensym="b" writesym="b"/>
    </state>
    <state name="halt"/>
  </states>
  

</turingmachine>
